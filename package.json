{
  "name": "mdip-did-eth-ledger",
  "version": "0.0.1",
  "description": "Smart contracts that implement MDIP DID smartcontract",
  "author": "Naveed",
  "keywords": [
    "MDIP",
    "DID",
    "ETH"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SelfKeyFoundation/selfkey-did-ledger.git"
  },
  "main": "truffle-config.js",
  "scripts": {
    "test": "truffle test",
    "test:cov": "solidity-coverage",
