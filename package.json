    "deploy-mainnet": "npm run _deploy mainnet"
  },
  "devDependencies": {
    "dotenv": "^8.0.0",
    "solhint": "^1.5.1",
    "solidity-coverage": "^0.6.0-beta.3",
    "truffle-hdwallet-provider": "1.0.6",
    "web3": "1.0.0-beta.37",
    "truffle": "5.1.14"
  },
  "dependencies": {
    "ethereumjs-util": "^6.1.0",
    "openzeppelin-solidity": "^2.2.0"
  }
